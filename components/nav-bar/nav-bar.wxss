
.navbar .nav{
  display: flex;
  height: 44px;
  align-items: center;
  color: #fff;
}
.nav .left,.nav .right{
  width: 120rpx
  
}
.nav .left,.nav .right,.nav .center{
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav .center{
  flex: 1;
}
.default{
  display: none;
}
.slot:empty + .default{
  display: flex;
}
.nav .left > .default{
  width: 36rpx;
}